        "accordion",
        "button",
        "checkbox",
        "color",
        "date",
        "datepicker",
        "email",
        "file",
        "grid",
        "hidden",
        "input",
        "image",
        "month",
        "multicheckbox",
        "number",
        "password",
        "radio",
        "range",
        "reset",
        "search",
        "select",
        "submit",
        "tel",
        "text",
        "time",
        "url",
        "week"
